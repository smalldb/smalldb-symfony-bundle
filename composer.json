{
    "name": "smalldb/smalldb-symfony-bundle",
    "description": "Symfony bundle for Smalldb",
    "license": "Apache-2.0",
    "type": "symfony-bundle",
    "homepage": "https://smalldb.org/",
    "authors": [
        {
            "name": "Josef Kufner",
            "email": "josef@kufner.cz"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://akvarko.frozen-doe.net/composer/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "require": {
        "php": ">=7.4",
        "smalldb/libsmalldb": "^2.0|dev-master",
        "symfony/config": ">=4.2",
        "symfony/dependency-injection": ">=4.2",
        "symfony/expression-language": ">=4.2",
        "symfony/http-kernel": ">=4.2",
        "symfony/var-dumper": ">=4.2"
    },
    "require-dev": {
        "smalldb/doc-tools": "dev-master",
        "smalldb/annotations": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Smalldb\\SmalldbBundle\\": ""
        }
    }
}
